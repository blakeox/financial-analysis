{
  "name": "@financial-analysis/analysis",
  "version": "0.1.1",
  "description": "Deterministic financial analysis engines",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config ./vitest.config.mjs",
    "test:coverage": "vitest --coverage --config ./vitest.config.mjs"
  },
  "dependencies": {
    "decimal.js": "^10.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.13.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
